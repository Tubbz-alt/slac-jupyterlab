# Preserve Jupyter variables
Defaults env_keep += "JPY_API_TOKEN JPY_BASE_URL JPY_COOKIE_NAME"
Defaults env_keep += "JPY_HUB_API_URL JPY_HUB_PREFIX JPY_USER"
Defaults env_keep += "JUPYTERHUB_API_TOKEN JUPYTERHUB_API_URL"
Defaults env_keep += "JUPYTERHUB_BASE_URL JUPYTERHUB_CLIENT_ID"
Defaults env_keep += "JUPYTERHUB_HOST JUPYTERHUB_OAUTH_CALLBACK_URL"
Defaults env_keep += "JUPYTERHUB_SERVICE_PREFIX JUPYTERHUB_USER"
Defaults env_keep += "JUPYTERLAB_IDLE_TIMEOUT"
